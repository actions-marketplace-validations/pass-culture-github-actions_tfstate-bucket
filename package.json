{
  "name": "changed-argocd-app",
  "version": "1.0.0",
  "description": "GitHub action to retrieve argocd app edited in a PR",
  "main": "lib/main.js",
  "scripts": {
    "build": "npm run build:main",
    "build:main": "ncc build src/main.ts -o lib",
    "lint": "prettier --write **/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pass-culture-github-actions/changed-argocd-app"
  },
