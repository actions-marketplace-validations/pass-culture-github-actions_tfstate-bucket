    "actions",
    "argocd",
    "pull-request",
    "apps"
  ],
  "author": "Dimitri Kopriwa <dimitri.kopriwa@passculture.app>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pass-culture-github-actions/changed-argocd-app/issues"
  },
  "homepage": "https://github.com/pass-culture-github-actions/changed-argocd-app#readme",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.0.0",
    "@actions/github": "^5.0.3",
    "@actions/io": "^1.1.3",
    "@actions/tool-cache": "^2.0.1",
    "execa": "^8.0.1"
  },
  "devDependencies": {
    "@tsconfig/node16": "^16.1.1",
    "@types/node": "^18.16.2",
    "@vercel/ncc": "^0.38.1",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
